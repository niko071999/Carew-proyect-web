<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/png" href="~/Assets/img/favicon.png">
    <title>@ViewBag.Title - Carew</title>
    <!-- jquery -->
    <script src="~/Scripts/jquery-3.5.1.js"></script>
    <!-- CSS only -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <!-- Custom styles for this template-->
    <link href="~/Content/css/sb-admin-2.min.css" rel="stylesheet" />
    <!-- The core Firebase JS SDK is always required and must be listed first -->
    <script src="https://www.gstatic.com/firebasejs/7.13.1/firebase-app.js"></script>
    <!-- JS CLEAVE -->
    <script src="https://cdn.jsdelivr.net/npm/cleave.js@1.6.0/dist/cleave.min.js" integrity="sha256-frGUwmSN4CLLjyk5m59ECdXsDMUxTW5O6hdceNHVCJo=" crossorigin="anonymous"></script>
    <!-- Custom fonts for this template-->
    <link href="~/Scripts/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <!-- Custom styles for tables -->
    <link href="~/Content/css/boleta.css" rel="stylesheet" />
    <link href="~/Content/Datatable/datatables.min.css" rel="stylesheet" />
    <link href="~/Content/Datatable/dataTables.bootstrap5.min.css" rel="stylesheet" />
    <link href="~/Content/Datatable/responsive.bootstrap5.min.css" rel="stylesheet" />
    <link href="~/Content/Datatable/buttons.bootstrap5.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/datetime/1.1.2/css/dataTables.dateTime.min.css" rel="stylesheet"/>
</head>
<body id="page-top">
    <audio id="audio_beep" controls>
        <source src="~/Assets/audio/beep.wav" />
    </audio>
    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav color-carew sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="@Url.Action("Index","Home")">
                <div class="sidebar-brand-icon">
                    <img class="img-fluid" src="~/Assets/logo/logo-web-carew.jpg" />
                </div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Dashboard -->
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Index","Home")">
                    <i class="fa fa-home"></i>
                    <span>Inicio</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Modulo de venta
            </div>

            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Listar","Venta")">
                    <i class="fas fa-money-bill-wave-alt"></i>
                    <span>Gestor de ventas</span>
                </a>
                <a class="nav-link" href="@Url.Action("POS","Venta")">
                    <i class="fas fa-cash-register"></i>
                    <span>Punto de venta</span>
                </a>
            </li>

            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Reportes de venta
            </div>

            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("ReporteVentaDiaria","Venta")">
                    <i class="far fa-file"></i>
                    <span>Reporte venta diaria</span>
                </a>
                <a class="nav-link" href="@Url.Action("ReporteProductosVendidos","Venta")">
                    <i class="far fa-file"></i>
                    <span>Reporte productos mas vendidos</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Modulo de producto
            </div>

            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Listar","Producto")">
                    <i class="fas fa-cube"></i>
                    <span>Gestor productos</span>
                </a>
                <a class="nav-link" href="@Url.Action("Listar","Categoria")">
                    <i class="fa fa-tags"></i>
                    <span>Gestor categorias</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Modulo de usuario
            </div>

            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link" href="@Url.Action("Listar","Usuario")">
                    <i class="fas fa-user"></i>
                    <span>Gestor usuarios</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>

        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                @RenderBody()
            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer fixed-bottom color-carew">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto text-white">
                        <span>Copyright &copy; CAREW 2022</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->
        </div>
    </div>
    <!-- End of Content Wrapper -->
    <!-- Custom scripts for all pages-->
    <script src="~/Scripts/js/sb-admin-2.min.js"></script>
    <!-- sweetalert2 - alerta mensajes -->
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <!-- Custom scripts for all users-->
    <script src="~/Scripts/js/alertmenssages.js"></script>
    <script src="~/Scripts/js/usersesion.js"></script>
    <script src="~/Scripts/js/scriptsUsuarios.js"></script>
    <!-- Custom scripts for tables -->
    <script src="~/Scripts/Datatable/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/Datatable/datatables.min.js"></script>
    <script src="~/Scripts/Datatable/dataTables.bootstrap5.min.js"></script>
    <script src="~/Scripts/Datatable/responsive.bootstrap5.min.js"></script>
    <script src="~/Scripts/Datatable/moment.min.js"></script>
    <script src="https://cdn.datatables.net/datetime/1.1.2/js/dataTables.dateTime.min.js"></script>
    <!-- Datatables Buttons Export -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.bootstrap5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.colVis.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.html5.min.js"></script>
    <script vsrc="https://cdn.datatables.net/buttons/2.2.3/js/buttons.print.min.js"></script>
    <!-- jsBarcode -->
    <script src="https://cdn.jsdelivr.net/npm/jsbarcode@3.11.0/dist/JsBarcode.all.min.js"></script>
    <script type="text/javascript">
        $('input').on('focus', function (e) {
            $(this)
                .one('mouseup', function () {
                    $(this).select();
                    return false;
                })
                .select();
        });
    </script>

</body>
</html>
